var myAge=21//creates variable myAge set to 21
var earlyYears=2//creates variable earlyYears set to 2
earlyYears*=10.5;
var laterYears=myAge-2;//create variable laterYears which subtracts 2 from myAge
laterYears*=4;//Mutiplied laterYears by 4
console.log(earlyYears,laterYears)
var myAgeInDogYears=earlyYears+laterYears;//creates variable myAgeInDogYears which adds earlyYears with laterYears
var myName='Ginge'.toLowerCase();//create myName makes the string lowercase
console.log(`My name is ${myName}. I am ${myAge} years old in human years which is ${myAgeInDogYears} years old in dog years.`)//prints the statement with the variables
